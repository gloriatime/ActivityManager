<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">

     <class name="UserTeam" table="t_user_team" >
       
       <composite-id>
       	<key-property name="user" type="int" column="member"></key-property>
       	<key-property name="team" type="int" column="team"></key-property>
       </composite-id>
        
    </class>
    
    
</hibernate-mapping>